	"main": "main.module.js",
	"scripts": {
		"tsc": "tsc",
		"start": "node src/main.module.js --max_old_space_size=4096",
		"dev": "nodemon src/main.module.ts",
		"dev:debug": "NODE_ENV=test nodemon --inspect=0.0.0.0:9229 --nolazy src/main.module.ts",
		"test": "set debug=* && ts-node-dev --respawn --transpileOnly ./node_modules/.bin/ts-node src/main.module.ts",
		"build": "./node_modules/.bin/tsc",
		"build:staging": "export NODE_ENV=staging && ./node_modules/.bin/tsc"
	},
